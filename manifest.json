{
  "manifest_version": 3,
  "name": "Google meet tracker",
  "description": "Tracking meet for teachers",
  "version": "1.0.5",
  "icons": {"128": "2.png"},
  "permissions": ["identity"],

  "content_scripts": [
    {
		"matches": [
            "https://meet.google.com/*"
        ],
        "html": "index.html",
        "js": [
            "script.js"
        ]
    }
],
  "action": {
    "default_icon": "1.png",
    "default_popup": "carcass.html"
  }

}